"""
A Company is a
"""
type Company {
    ID: Int @extraTag(gorm:"primarykey")
    createdAt: Time
    updatedAt: Time
    name: String!
    logoID: Int
    logo: Asset @extraTag(gorm:"foreignKey:LogoID")
    description: String
    tel: String!
    userID: Int
    user: User! @extraTag(gorm:"foreignKey:UserID")
    verified: Boolean
}

input CompanyInput {
    name: String!
    logo: String
    description: String!
    tel: String!
}
